{
  "name": "catchie",
  "version": "1.0.2",
  "description": "Retries a provided function n times on error",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "./node_modules/.bin/eslint .",
    "cover": "CONSOLE_LOGGING=false node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- test --recursive --timeout=20000 --exit",
    "test": "npm run lint && npm run cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dial-once/node-catchie.git"
  },
  "keywords": [
    "error",
    "catch",
    "catchie",
    "retry",
    "repeat",
    "repeater"
  ],
  "author": "Dial-Once",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dial-once/node-catchie/issues"
  },
  "homepage": "https://github.com/dial-once/node-catchie#readme",
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "sinon": "^2.3.1"
  }
}
